docker run -it \
	--gpus all \
	-e DISPLAY=$DISPLAY \
	-v $XAUTH:/root/.Xauthority \
	-v /tmp/.X11-unix:/tmp/.X11-unix:rw \
	--volume $HOME/docker/:/root/my_workspace/ \
